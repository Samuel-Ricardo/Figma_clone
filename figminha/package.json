{
  "name": "figminha",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "next dev",
    "build": "next build",
    "build:clean": "npm run code:ci && npm run build",
    "start": "next start",
    "start:docker": "npm run build && npm run start",
    "lint": "next lint",
    "lint:staged": "eslint --fix --cache **/*.{js,jsx,ts,tsx}",
    "format:verify": "prettier --check --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx}",
    "format:fix": "prettier --write --ignore-path .gitignore ./src/**/*.{js,jsx,ts,tsx}",
    "husky:prepare": "cd ../ && husky install",
    "test": "jest --passWithNoTests --color",
    "test:coverage": "jest --coverage --passWithNoTests --color",
    "test:watch": "jest --watchAll --coverage --passWithNoTests --color",
    "test:staged": "jest --passWithNoTests --color --findRelatedTests",
    "test:integration": "jest --config jest-e2e.config.mjs",
    "test:E2E:": "cypress run",
    "test:cypress": "cypress open",
    "code:ci": "npm run format:fix && npm run lint && npm run test:coverage"
  },
  "dependencies": {
    "inversify": "^6.1.4",
    "inversify-inject-decorators": "^3.1.0",
    "next": "15.0.3",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "reflect-metadata": "^0.2.2",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cypress": "^13.15.2",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
